{
	"name": "@toreda/adt",
	"version": "0.4.0",
	"description": "TypeScript Data Structures.",
	"public": true,
	"main": "./dist/index.js",
	"typings": "./dist/index.d.ts",
	"scripts": {
		"lint": "yarn eslint ./src/** --quiet",
		"lint:nofix": "yarn lint",
		"lint:fix": "yarn eslint src/**/*.{js,ts,tsx} --quiet --fix",
		"make:docs": "yarn typedoc docs --categorizeByGroup false --entryPoints ./src/index.ts",
		"build": "yarn gulp",
		"test": "yarn jest --coverage"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/toreda/adt.git"
	},
	"keywords": [
		"TypeScript",
		"data",
		"structures",
		"adt",
		"abstract data type",
		"data structures",
		"queue",
		"typescript queue",
		"stack",
		"typescript stack",
		"circular queue",
		"typescript circular queue",
		"object pool",
		"typescript object pool",
		"priority queue",
		"typescript priority queue",
		"generic data structure",
		"generics",
		"linked list",
		"iterable",
		"linked-list",
		"typescript linked list",
		"typescript linked-list"
	],
	"author": "Toreda, Inc.",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/toreda/adt/issues"
	},
	"prettier": "@toreda/prettier-config",
	"homepage": "https://github.com/toreda/adt#readme",
	"devDependencies": {
		"@toreda/build-tools": "^0.5.7",
		"@toreda/eslint-config": "^2.2.0",
		"@toreda/prettier-config": "^1.0.1",
		"@types/gulp": "^4.0.9",
		"@types/jest": "^27.0.2",
		"@types/node": "^16.11.6",
		"@typescript-eslint/parser": "^5.3.0",
		"@typescript-eslint/typescript-estree": "^5.3.0",
		"eslint": "^8.2.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"jest": "^27.3.1",
		"jest-sonar-reporter": "^2.0.0",
		"prettier": "^2.4.1",
		"ts-jest": "^27.0.7",
		"ts-loader": "^9.2.6",
		"ts-node": "^10.4.0",
		"typedoc": "^0.22.8",
		"typescript": "^4.4.4"
	},
	"dependencies": {
		"@toreda/log": "^0.6.5"
	}
